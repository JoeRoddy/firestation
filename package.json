{
  "name": "firestation-electron",
  "version": "0.1.0",
  "description": "Execute SQL Queries Against Your Firestation Database",
  "productName": "Firestation",
  "main": "main.js",
  "scripts": {
    "lint": "eslint app test *.js",
    "hot-server": "node -r babel-register server.js",
    "build-main": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.electron.js --progress --profile --colors",
    "build-renderer": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.prod.js --progress --profile --colors",
    "build": "npm run build-main && npm run build-renderer",
    "start": "cross-env NODE_ENV=production electron ./",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron -r babel-register -r babel-polyfill ./main.development",
    "package": "cross-env NODE_ENV=production node -r babel-register package.js",
    "package-all": "npm run package -- --all",
    "dev": "concurrently --kill-others \"npm run hot-server\" \"npm run start-hot\""
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "license": "MIT",
  "devDependencies": {
    "asar": "^0.12.3",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-syntax-decorators": "^6.13.0",
    "babel-plugin-syntax-flow": "^6.13.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-plugin-webpack-loaders": "^0.7.1",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "concurrently": "^2.2.0",
    "cross-env": "^2.0.1",
    "css-loader": "^0.26.1",
    "del": "^2.2.2",
    "devtron": "^1.3.0",
    "electron": "^1.8.8",
    "electron-devtools-installer": "^2.0.1",
    "electron-packager": "^7.7.0",
    "electron-rebuild": "^1.2.0",
    "electron-remote": "^1.0.8",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-import-resolver-webpack": "^0.5.1",
    "eslint-plugin-flowtype": "^2.11.4",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^6.2.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "fbjs-scripts": "^0.7.1",
    "flow": "^0.2.3",
    "flow-bin": "^0.31.1",
    "json-loader": "^0.5.4",
    "minimist": "^1.2.6",
    "node-libs-browser": "^1.0.0",
    "node-sass": "^3.4.2",
    "prettier": "^1.5.2",
    "react-addons-test-utils": "^15.3.1",
    "sass-loader": "^3.2.0",
    "spectron": "^3.3.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2",
    "webpack-merge": "^0.14.1"
  },
  "dependencies": {
    "aws-sdk": "^2.154.0",
    "electron-debug": "^1.0.1",
    "firebase": "^4.6.2",
    "firebase-admin": "^5.5.0",
    "jquery": "^3.4.0",
    "lodash": "^4.17.13",
    "mobx": "^2.4.4",
    "mobx-react": "^3.5.5",
    "mobx-react-devtools": "^4.2.5",
    "moment": "^2.19.3",
    "react": "^15.3.1",
    "react-ace": "^4.3.0",
    "react-dom": "^15.3.1",
    "react-tooltip": "^3.3.0",
    "source-map-support": "^0.4.2",
    "type-name": "^2.0.2"
  }
}
